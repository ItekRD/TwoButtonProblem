/ {
	chosen {
		nordic,nus-uart = &uart0;
	};
};

&uart1 {
	status = "okay";
};

&spi1 {
	status = "disabled";
};

&spi3 {
	status = "disabled";
};

&qspi {
	status = "disabled";
};

&button3 {
	gpios = <&gpio0 25 0>;
};

&led3 {
	gpios = <&gpio0 16 0>;
};

&led0 {
	gpios = <&gpio0 13 0>;
};
